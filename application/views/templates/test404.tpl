        <object type="application/x-shockwave-flash" name="name" data="{asset_url()}/pacman.swf" width="977" height="500" id="flash-404" style="visibility: visible;"><param name="quality" value="high"><param name="wmode" value="transparent"></object>
    </div>
    
</div>
